-     t_real = x_real[i] + x_real[ip];
-     t_imag = x_imag[i] + x_imag[ip];
-     x_real[ip] = x_real[i] - x_real[ip];
-     x_imag[ip] = x_imag[i] - x_imag[ip];
-     x_real[i] = t_real;
-     x_imag[i] = t_imag;
