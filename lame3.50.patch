- 
- /*************************************************************************/
- /*            choose table                                               */
- /*************************************************************************/
- 
- int choose_table( int max )
- {
-     int  i, choice;
- 
-     if ( max == 0 )
-         return 0;
-     
-     max = abs( max );    
-     choice = 0;
- 
-     if ( max < 15 )
-     {
-         for ( i = 0; i < 15; i++ )
-         {
-             if ( ht[i].xlen > max )
-             {
- 		choice = i;
- 		break;
-             }
-         }
-     }
-     else
-     {	
- 	max -= 15;
- 	for (i = 15; i < 32; i++ )
- 	{
- 	    if ( ht[i].linmax >= max )
- 	    {
- 		choice = i;
- 		break;
- 	    }
- 	}
-     }
-     assert( choice );
-     return choice;
- }
