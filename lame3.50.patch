- void calc_xmin( double xr[2][2][576], III_psy_ratio *ratio,
- 	   gr_info *cod_info, III_psy_xmin *l3_xmin,
- 	   int gr, int ch )
- {
-     int start, end, sfb, l, b;
-     double en, bw;
