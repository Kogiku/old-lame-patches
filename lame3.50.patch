+ static void analyze(void)
+ {
+     if ( idle_keepgoing) {
+       idle_count = 0;
+       idle_count_max=0;
+       idle_keepgoing=0;
+       idle_finish=0;
+     }
+     plot_frame();   
+     update_progress(); 
+ }
