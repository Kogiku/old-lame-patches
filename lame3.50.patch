-        le = 1 << (M-L);
-        le1 = le >> 1;
-        w_real[0][L] = cos(PI/le1);
-        w_imag[0][L] = -sin(PI/le1);
-     }          
-     M = LOGBLKSIZE_S;
-     for(L=0; L<M; L++){
-        le = 1 << (M-L);
-        le1 = le >> 1;
-        w_real[1][L] = cos(PI/le1);
-        w_imag[1][L] = -sin(PI/le1);
-     }          
-     init++;
-  }
-  switch(N) {
- 	case BLKSIZE:
- 			M = LOGBLKSIZE;
- 			MP = 0;
- 			break;
- 	case BLKSIZE_S:
- 			M = LOGBLKSIZE_S;
- 			MP = 1;
- 			break;
- 	default:	printf("Error: Bad FFT Size in subs.c\n");
