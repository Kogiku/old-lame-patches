-      for(i = 0; i< SBLIMIT; ++i) {
-        if(mnr[k][i] < NOISY_MIN_MNR)   ++noisy_sbs;
-        if(small > mnr[k][i])           small = mnr[k][i];
-      }
-    }
-    return noisy_sbs;
- }
