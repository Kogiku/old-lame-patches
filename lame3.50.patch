+ #ifdef _MSC_VER
+ #define MSVC_XRPOW_ASM
+ #ifdef MSVC_XRPOW_ASM
+ # define MSVC_FTOL(src, dest) do { \
+     double src_ = (src); \
+     int dest_; \
+     { \
+       __asm fld src_ \
+       __asm fistp dest_ \
+     } \
+     (dest) = dest_; \
+   } while (0)
+ # endif
+ #endif
