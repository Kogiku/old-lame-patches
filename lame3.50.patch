+ iteration_loop( double pe[2][2], double ms_ener_ratio[2],
+ 		double xr_org[2][2][576], III_psy_ratio *ratio,
+ 		III_side_info_t *l3_side, int l3_enc[2][2][576],
+ 		III_scalefac_t *scalefac, frame_params *fr_ps)
+ {
+   III_psy_xmin l3_xmin;
