+ /*************************************************************************/
+ 
+ static int ix_max_takehiro(int *ix, int *end) 
+ {
+     int max = 0;
+ 
+     while (ix < end) {
+ 	int x =	 *ix++;
+ 	if (max < x) 
+ 	    max = x;
+     }
+     return max;
+ }
