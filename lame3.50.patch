-  *         #define      PI          3.14159265358979                         *
-  *         #define      BLKSIZE     1024                                     *
-  *         #define      LOGBLKSIZE  10                                       *
-  *         #define      BLKSIZE_S   256                                      *
-  *         #define      LOGBLKSIZE_S 8                                       *
-  *                                                                           *
-  *****************************************************************************/
- #define      BLKSIZE_S   256
- #define      LOGBLKSIZE_S 8
- 
- void fft(x_real,x_imag, energy, phi, N)
- FLOAT x_real[BLKSIZE], x_imag[BLKSIZE], energy[BLKSIZE], phi[BLKSIZE];
- int	N;
- {
-  int     M,MM1;
-  static int     init=0;
-  int     NV2, NM1, MP;
