+   layer *info;
+   int VBRbits[2][2];
+   int over[2];
+   double noise[4]; /* over,max_noise,over_noise,tot_noise; */
+   double targ_noise[4]; /* over,max_noise,over_noise,tot_noise; */
+   int bitsPerFrame;
