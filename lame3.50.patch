- 
-     for ( sfb = 0; sfb < cod_info->sfb_lmax; sfb++ )
-     {
-         start = scalefac_band_long[ sfb ];
-         end   = scalefac_band_long[ sfb+1 ];
- 	bw = end - start;
- 
-         for ( en = 0.0, l = start; l < end; l++ )
-             en += xr[gr][ch][l] * xr[gr][ch][l];
-         l3_xmin->l[gr][ch][sfb] = ratio->l[gr][ch][sfb] * en / bw;
