+ 	     f0      = fi[0 ] + fi[k1];
+ 	     f3      = fi[k2] - fi[k3];
+ 	     f2      = fi[k2] + fi[k3];
+ 	     fi[k2]  = f0	  - f2;
+ 	     fi[0 ]  = f0	  + f2;
+ 	     fi[k3]  = f1	  - f3;
+ 	     fi[k1]  = f1	  + f3;
+ 	     g1      = gi[0 ] - gi[k1];
+ 	     g0      = gi[0 ] + gi[k1];
+ 	     g3      = SQRT2  * gi[k3];
+ 	     g2      = SQRT2  * gi[k2];
+ 	     gi[k2]  = g0	  - g2;
+ 	     gi[0 ]  = g0	  + g2;
+ 	     gi[k3]  = g1	  - g3;
+ 	     gi[k1]  = g1	  + g3;
+ 	     gi     += k4;
+ 	     fi     += k4;
+ 	    } while (fi<fn);
+      t_c = costab[k];
+      t_s = sintab[k];
+      c1 = 1;
+      s1 = 0;
+      for (i=1;i<kx;i++)
+         {
+ 	 FLOAT c2,s2;
+          FLOAT t = c1;
+          c1 = t*t_c - s1*t_s;
+          s1 = t*t_s + s1*t_c;
+          c2 = c1*c1 - s1*s1;
+          s2 = 2*(c1*s1);
+ 	     fn = fz + n;
+ 	     fi = fz +i;
+ 	     gi = fz +k1-i;
+ 	     do
+ 		{
+ 		 FLOAT a,b,g0,f0,f1,g1,f2,g2,f3,g3;
+ 		 b       = s2*fi[k1] - c2*gi[k1];
+ 		 a       = c2*fi[k1] + s2*gi[k1];
+ 		 f1      = fi[0 ]    - a;
+ 		 f0      = fi[0 ]    + a;
+ 		 g1      = gi[0 ]    - b;
+ 		 g0      = gi[0 ]    + b;
+ 		 b       = s2*fi[k3] - c2*gi[k3];
+ 		 a       = c2*fi[k3] + s2*gi[k3];
+ 		 f3      = fi[k2]    - a;
+ 		 f2      = fi[k2]    + a;
+ 		 g3      = gi[k2]    - b;
+ 		 g2      = gi[k2]    + b;
+ 		 b       = s1*f2     - c1*g3;
+ 		 a       = c1*f2     + s1*g3;
+ 		 fi[k2]  = f0        - a;
+ 		 fi[0 ]  = f0        + a;
+ 		 gi[k3]  = g1        - b;
+ 		 gi[k1]  = g1        + b;
+ 		 b       = c1*g2     - s1*f3;
+ 		 a       = s1*g2     + c1*f3;
+ 		 gi[k2]  = g0        - a;
+ 		 gi[0 ]  = g0        + a;
+ 		 fi[k3]  = f1        - b;
+ 		 fi[k1]  = f1        + b;
