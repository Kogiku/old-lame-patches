! /* This is a simplified version for n an even power of 2 */
! 
! static void fht(FLOAT *fz, int n)
  {
!  int i,k,k1,k2,k3,k4,kx;
!  FLOAT *fi,*fn,*gi;
!  FLOAT t_c,t_s;
! 
!  for (k1=1,k2=0;k1<n;k1++)
!     {
!      FLOAT a;
!      for (k=n>>1; (!((k2^=k)&k)); k>>=1);
