-  for(i=0;i<HAN_SIZE;i++){    /* calculate power density spectrum */
-     if (energy[i] < 1E-20) energy[i] = 1E-20;
-     power[i].x = 10 * log10(energy[i]) + POWERNORM;
-     power[i].next = STOP;
-     power[i].type = FALSE;
