+      {
+       FLOAT f0,f1,f2,f3;
+       f1     = fi[0 ]-fi[1 ];
+       f0     = fi[0 ]+fi[1 ];
+       f3     = fi[2 ]-fi[3 ];
+       f2     = fi[2 ]+fi[3 ];
+       fi[2 ] = (f0-f2);	
+       fi[0 ] = (f0+f2);
+       fi[3 ] = (f1-f3);	
+       fi[1 ] = (f1+f3);
+      }
+ 
+  k=0;
+  do
+     {
+      FLOAT s1,c1;
+      k  += 2;
+      k1  = 1  << k;
+      k2  = k1 << 1;
+      k4  = k2 << 1;
+      k3  = k2 + k1;
+      kx  = k1 >> 1;
+ 	 fi  = fz;
+ 	 gi  = fi + kx;
+ 	 fn  = fz + n;
+ 	 do
+ 	    {
+ 	     FLOAT g0,f0,f1,g1,f2,g2,f3,g3;
